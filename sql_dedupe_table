%sql
-- Step 1: Create a new table with distinct rows to remove duplicates
CREATE TABLE db.temp_unique_table AS
SELECT DISTINCT *
FROM hive_metastore.db.budgets;
-- Step 2: Overwrite the original table with deduplicated data
INSERT OVERWRITE TABLE hive_metastore.db.budgets
SELECT * FROM db.temp_unique_table;

-- Step 3: Drop the temporary table if no longer needed
DROP TABLE IF EXISTS db.temp_unique_table;
